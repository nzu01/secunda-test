{
  "openapi": "3.0.3",
  "info": {
    "title": "Organizations API",
    "version": "1.0.0",
    "description": "REST API для работы с организациями, зданиями и типами организаций.\nВо всех примерах для коллекций возвращён **один объект** для краткости.\n"
  },
  "servers": [
    {
      "url": "http://localhost:8280"
    }
  ],
  "tags": [
    {
      "name": "Organizations",
      "description": "Методы работы с организациями"
    },
    {
      "name": "Buildings",
      "description": "Методы работы со зданиями"
    },
    {
      "name": "Organization Types",
      "description": "Методы работы с типами организаций"
    }
  ],
  "components": {
    "parameters": {
      "OffsetParam": {
        "name": "offset",
        "in": "query",
        "description": "Смещение выборки",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 0,
          "default": 0
        }
      },
      "LimitParam": {
        "name": "limit",
        "in": "query",
        "description": "Лимит записей",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 1,
          "maximum": 100,
          "default": 20
        }
      }
    },
    "schemas": {
      "DateTimeISO": {
        "type": "string",
        "format": "date-time",
        "example": "2025-09-05T20:41:00+00:00"
      },
      "UUID": {
        "type": "string",
        "format": "uuid",
        "example": "63587d49-ee3c-499e-a260-5c0dd39ae133"
      },
      "Building": {
        "type": "object",
        "required": [
          "id",
          "uuid",
          "address",
          "latitude",
          "longitude",
          "created_at",
          "updated_at"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 20
          },
          "uuid": {
            "$ref": "#/components/schemas/UUID"
          },
          "address": {
            "type": "string",
            "example": "ул. Нобеля, 7, Москва, 143026"
          },
          "latitude": {
            "type": "number",
            "format": "float",
            "example": 55.698697
          },
          "longitude": {
            "type": "number",
            "format": "float",
            "example": 37.368379
          },
          "created_at": {
            "$ref": "#/components/schemas/DateTimeISO"
          },
          "updated_at": {
            "$ref": "#/components/schemas/DateTimeISO"
          }
        }
      },
      "OrganizationTypePivot": {
        "type": "object",
        "properties": {
          "organization_uuid": {
            "$ref": "#/components/schemas/UUID"
          },
          "organization_type_uuid": {
            "$ref": "#/components/schemas/UUID"
          }
        }
      },
      "OrganizationType": {
        "type": "object",
        "required": [
          "uuid",
          "title",
          "name",
          "children"
        ],
        "properties": {
          "uuid": {
            "$ref": "#/components/schemas/UUID"
          },
          "title": {
            "type": "string",
            "example": "Еда"
          },
          "name": {
            "type": "string",
            "example": "food"
          },
          "children": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrganizationType"
            }
          }
        }
      },
      "OrganizationTypeWithPivot": {
        "allOf": [
          {
            "$ref": "#/components/schemas/OrganizationType"
          },
          {
            "type": "object",
            "properties": {
              "pivot": {
                "$ref": "#/components/schemas/OrganizationTypePivot"
              }
            }
          }
        ]
      },
      "OrganizationContact": {
        "type": "object",
        "required": [
          "id",
          "uuid",
          "organization_uuid",
          "contact_type",
          "value",
          "created_at",
          "updated_at"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 1457
          },
          "uuid": {
            "$ref": "#/components/schemas/UUID"
          },
          "organization_uuid": {
            "$ref": "#/components/schemas/UUID"
          },
          "contact_type": {
            "type": "string",
            "example": "phone"
          },
          "value": {
            "type": "string",
            "example": "+1.628.395.2762"
          },
          "created_at": {
            "$ref": "#/components/schemas/DateTimeISO"
          },
          "updated_at": {
            "$ref": "#/components/schemas/DateTimeISO"
          }
        }
      },
      "Organization": {
        "type": "object",
        "required": [
          "id",
          "uuid",
          "title",
          "building_uuid",
          "created_at",
          "updated_at",
          "building",
          "types",
          "organization_contacts"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 981
          },
          "uuid": {
            "$ref": "#/components/schemas/UUID"
          },
          "title": {
            "type": "string",
            "example": "Incidunt magnam incidunt."
          },
          "building_uuid": {
            "$ref": "#/components/schemas/UUID"
          },
          "created_at": {
            "$ref": "#/components/schemas/DateTimeISO"
          },
          "updated_at": {
            "$ref": "#/components/schemas/DateTimeISO"
          },
          "building": {
            "$ref": "#/components/schemas/Building"
          },
          "types": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrganizationTypeWithPivot"
            }
          },
          "organization_contacts": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrganizationContact"
            }
          }
        }
      },
      "OrganizationsResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Organization"
            }
          }
        }
      },
      "OrganizationResponse": {
        "type": "object",
        "properties": {
          "data": {
            "$ref": "#/components/schemas/Organization"
          }
        }
      },
      "BuildingsResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Building"
            }
          }
        }
      },
      "OrganizationTypesResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrganizationType"
            }
          }
        }
      }
    }
  },
